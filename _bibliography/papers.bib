@article{park2024a,
	title = {Examining prevalence and predictors of food insecurity for transition-age youth transitioning out of foster care},
	volume = {Preprint},
	issn = {1554-8732},
	url = {https://doi.org/10.1080/15548732.2024.2332611},
	doi = {10.1080/15548732.2024.2332611},
	abstract = {Transition-age youth with foster care involvement (TAY) face significant risks for food insecurity and other hardships in early adulthood. Using representative survey data of youth transitioning out of foster care in California, we examine the prevalence and predictors of food insecurity. We find that about 30\% of study participants were food insecure at ages 19, 21, and 23. We also identify multiple risk and protective factors associated with being food insecure, such as TAY’s sexual identity and receipt of public benefits. The results of our study offer life-stage-specific recommendations for policy and practice to address food insecurity among TAY.},
	urldate = {2024-05-23},
	journal = {Journal of Public Child Welfare},
	author = {Park, Sunggeun and Nadon, Melanie and Okpych, Nathanael J. and Harty, Justin S. and Courtney, Mark E.},
	year = {2024},
	note = {cv-pub-journal},
	keywords = {cv-pub-journal},
}

@article{harty2025c,
	title = {The institutional web: {Fathers} of color navigating child welfare and intersecting systems},
	volume = {102},
	abstract = {This scoping review examines the challenges fathers of color face at the intersections of the child welfare system and family support, child support, and criminal justice systems. By reviewing literature from 2008 to 2023, significant gaps were identified in how these systems impact these fathers. The study highlights the need for expanded empirical research, culturally competent interventions, and policy reforms to address systemic inequities and improve outcomes for fathers and their families.},
	number = {5},
	journal = {Child Welfare},
	author = {Harty, Justin S. and Walsh, Tova B. and Lee, Shawna J. and Charles, Pajarita and Pate, David},
	year = {2025},
	note = {cv-pub-journal-inpress},
	keywords = {cv-pub-journal-inpress},
}

@article{lee2024,
	title = {Role of fathers in the well-being of children impacted by the {U}.{S}. child welfare system: {Recommendations} for practice and policy from a scoping review},
	abstract = {The goal of this scoping review was to summarize and synthesize empirical evidence on the link between father involvement and child well-being among children impacted by the U.S. child welfare system. Drawing from the ecological systems framework of investigating father-child relationships, we focused on multi-level socioecological factors that may shape father-child relationships and child outcomes. Results showed that the relevant evidence base is small (N = 9 studies), with studies showing generally mixed findings concerning the links between father involvement and child well-being. This work calls for more attention and engagement of fathers in child welfare research.},
	journal = {Child Welfare},
	author = {Lee, Joyce Y. and Yoon, Susan and Harty, Justin S. and Chang, Yujeong and Cha, Hunmin and Xu, Amy},
	year = {2024},
	note = {cv-pub-journal-inpress},
	keywords = {cv-pub-journal-inpress},
}

@article{lechuga-pena2024,
	title = {Forging futures, building a pipeline: {Testimonios} from a collective of diverse {MFP} alums in the southwest},
	volume = {Preprint},
	doi = {10.1080/10437797.2024.2400064},
	abstract = {Over its 50-year history, the Minority Fellowship Program (MFP) has focused on increasing the number of practitioners and scholars trained to serve underrepresented and underserved racial and ethnic minority communities at risk of mental health and substance abuse issues. With over 1,000 individuals trained at the master’s and doctoral levels, the MFP has played a signicant role in preparing scholar-practitioners to make impactful contributions in addressing the needs of vulnerable populations. Through testimonios, we explore the career trajectory, collaborative partnerships, and community effect of a cluster of five doctoral-level MFP alums hired at a large public R1 university in the Southwest from 2016 to 2023. Our testimonios bear witness to our collective experiences as MFP alums and diverse faculty of color, particularly as they relate to our positionality before, during, and after our time as fellows. We examine the pivotal aspects of MFP participation, including its infuence on our engagement in antiracist, antioppressive, and decolonial initiatives and efforts to recruit and mentor graduate students of color. We close by sharing the enduring lessons and insights gained from the MFP and propose strategies for cultivating a diverse doctoral student pipeline in social work to address mental health and substance abuse issues.},
	journal = {Journal of Social Work Education},
	author = {Lechuga-Peña, Stephanie and Mitchell, Felicia M. and Ignacio, Matt and Harty, Justin S. and Billiot, Shanondora M.},
	year = {2024},
	note = {cv-pub-journal},
	keywords = {cv-pub-journal},
}

@article{harty2024j,
	title = {Origins of opportunity, part 1: {The} formation of {CSWE}’s minority research fellowship program, 1954–1978},
	volume = {Preprint},
	doi = {10.1080/10437797.2024.2407826},
	abstract = {This paper explores the historical development of the Council on Social Work
Education’s (CSWE) Minority Fellowship Program (MFP), coinciding with the 50th anniversary
of its research fellowship. By leveraging archival CSWE materials like meeting minutes, reports,
letters, and newsletters, it chronicles key events from 1954 that culminated in the MFP’s creation
in 1973 and extends to the third research fellowship cohort in 1978, just before the introduction
of the second clinical fellowship. The paper details the challenges and milestones that shaped
early initiatives for diversity and inclusion within social work education. It offers a reflective
examination of the MFP’s inception, tracing the advocacy and structural changes within the
CSWE and broader social movements that culminated in the creation of the MFP and its
sustained impact on the profession. Established to address the underrepresentation of minorities
in social work education, the MFP was designed to develop researchers and practitioners of color
attuned and responsive to the varied needs of their communities. This paper not only enriches the
social work historical record but also highlights the transformative impact of the MFP in
cultivating a profession that truly reflects and effectively serves our racially and ethnically
diverse society.},
	journal = {Journal of Social Work Education},
	author = {Harty, Justin S. and BlackDeer, Autumn Asher and Schiele, Jerome H. and Francis, E. Aracelis and Washington, Durrell M.},
	year = {2024},
	note = {cv-pub-journal},
	keywords = {cv-pub-journal},
}


@misc{2023d,
	address = {Social Welfare History Group, Virtual},
	type = {Webinar co-organizer},
	title = {Red scares, political repression, and social work: {Yesterday}, today, and tomorrow},
	url = {https://youtu.be/x4IVwpxn8ws},
	abstract = {The American red scares of the twentieth century point to an evolving political landscape rather than isolated bouts of hysteria that exploded onto—and then vanished—from the political scene. Under the guise of "protecting democracy," organized political forces stifled and repressed progressive social policy, political and social activity, and democratic and constitutional rights. Thousands of ordinary people were ostracized, facing material consequences including imprisonment, deportation, loss of employment, and organized violence. As we will discuss in this webinar, red scares did not exempt social workers—nor have we seen the last one. Panelists: Mimi Abramovitz, Silberman SSW, Hunter College CUNY; Laura Curran, Rutgers University School of Social Work; Jessica Toft, University of Minnesota School of Social Work; Kit Ginzky, joint-PhD candidate, University of Chicago Crown Family School of Social Work and Department of History; Jane McPherson, Associate Professor and Director of Global Engagement, University of Georgia School of Social Work.},
	month = apr,
	year = {2023},
	note = {cv-pres-organized},
	keywords = {cv-pres-organized},
}

@misc{2022c,
	address = {Social Welfare History Group, Virtual},
	type = {Webinar co-organizer and panelist},
	title = {Doctoral student interested in (“making”) history},
	abstract = {On April 8, 2022, the Social Welfare History Group (SWHG) leadership team hosted “A Dialogue and Call to Action” event attended by 130 social work educators. The 272 registered participants represented 32 states, 3 countries, and several scholarly disciplines. The discussion highlighted the importance of preparing history-informed doctoral students to (1) conduct historical research that will advance and correct the existing historical record, (2) teach history to BSW and MSW students, and (3) advance social work practice that is historically informed. In response to strong requests from Social Welfare and Social Work doctoral students for the opportunity to discuss their interest in historical research with other students and faculty, the SWHG has planned the following first-ever meeting of Doctoral Students Interested In (“Making”) History. We welcome students interested in: Support for an existing historical dissertation research; Developing an initial historical dissertation research topic; Adding historical content to dissertation, class, assignment, publications; Teaching a social work history course; Pursuing historical knowledge foundational to the social work canon. Panelists: Justin S. Harty, Assistant Professor, School of Social Work, Arizona State University; Mimi Abramovitz, Bertha Capen Reynolds Professor of Social Policy Emerita, Silberman School of Social Work, Hunter College, CUNY; Stephen M. Tomczak, Professor of Social Welfare Policy \& Community Organization, Department of Social Work, Southern Connecticut State University},
	month = oct,
	year = {2022},
	note = {cv-pres-organized},
	keywords = {cv-pres-organized},
}

@misc{harty2023e,
	address = {Group for the Advancement of Doctoral Education in Social Work 2023 Annual Conference, Arlington, TX},
	type = {Invited keynote speaker},
	title = {Social work history: {Lessons} for doctoral education in divisive times},
	url = {https://www.prof2prof.com/resource/social-work-history-lessons-doctoral-education-divisive-times-slides-speaker-notes},
	abstract = {In this keynote presentation, we will delve into the complex history of the social work profession and its relationship to white supremacy, colonialism, and racism. Drawing on lessons from our shared history, we will explore how social work has both perpetuated and responded to historical atrocities between the 1860s and today, with a particular focus on the ethical dilemmas that social workers have faced when decisions of conscience challenge the law and professional values. We will discuss the resistance efforts made by historically marginalized populations and the social work profession itself against inequality in pursuit of social and racial justice. By understanding the origins of the term "social work" and what it means to populations historically left out of dominant Eurocentric social work efforts, we can equip social work doctoral students with the knowledge and skills necessary to navigate such complex ethical dilemmas in promoting a more civil, equitable, and just society. This keynote presentation is an important forum for purposeful dialogue on how we can draw on the lessons of social work history to prepare doctoral students for the challenges of today's divisive times.},
	author = {Harty, Justin S},
	month = mar,
	year = {2023},
	note = {cv-conf-keynote},
	keywords = {cv-conf-keynote},
}

@misc{harty2023f,
	address = {Group for the Advancement of Doctoral Education in Social Work 2023 Annual Conference, Arlington, TX},
	type = {Invited workshop organizer and discussant},
	title = {Leveraging social work history in divisive times: {A} call to action for faculty to transform doctoral education},
	url = {https://www.prof2prof.com/resource/social-work-history-lessons-doctoral-education-divisive-times-slides-speaker-notes},
	abstract = {This session will explore the importance of social work history in today's divisive times. The main focus of this session will be on how faculty can leverage this history to bring about positive change. Attendees will get a deeper understanding of the historical roots of social work, including the profession's negative impact on marginalized communities, as well as the contributions of social workers and communities of color that have been erased from the profession's history. The session aims to inspire faculty to incorporate social work history into their doctoral education curricula and to use this history as a catalyst for social justice activism and action.},
	author = {Harty, Justin S},
	month = mar,
	year = {2023},
	note = {cv-conf-roundtables},
	keywords = {cv-conf-roundtables},
}


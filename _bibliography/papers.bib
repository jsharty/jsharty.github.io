@article{blackdeer2025,
	title = {Beyond reform and abolition: {Navigating} the complexities of indigenous child welfare},
	abstract = {Despite the passage of the Indian Child Welfare Act nearly 50 years ago, Indigenous children are persistently disproportionately represented within the child welfare system yet continue to be overlooked in the debate between abolition and reform in the social work literature. Purpose: As these disparities continue to widen, social workers fail to recognize the unique struggles of Indigenous children and families upon encountering the child welfare system. Design: The present work provides a historic overview of Indigenous child welfare interaction, grounds discussions in coloniality, and implicates social work’s complicity in perpetuating the genocidal fiscal economy of Native child trafficking. Results: Through a lens of Indigenous child welfare, we sift through the abolitionist/reformist debate, introduce the Indian Child Welfare Act as an example of a non- reformist reform, concluding with implications for social work practice.},
	journal = {Families in Society},
	author = {BlackDeer, Autumn Asher and Harty, Justin S.},
	year = {2025},
	note = {cv-pub-journal-inpress},
	keywords = {cv-pub-journal-inpress},
}
